module constraints

imports
	include/MetaC
	lib/runtime/nabl/-
	lib/runtime/task/-
  	lib/runtime/types/-
  	lib/runtime/relations/-
	libstratego-lib
	lib/editor-common.generated
	
	
rules
		
	nabl-check-disable-duplicate(|uri, ns) = id
  	nabl-check-disable-hiding(|uri, ns) = id
  	
  	nabl-constraint(|ctx) :
    TypedefDecl(Type(mod, type), name) -> <fail>
    where 
      <last> mod;
      <task-create-warning(|ctx, ["Type qualifiers are meaningless in this declaration."])> mod